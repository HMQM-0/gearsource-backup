    "alt": "cross-env API_URI=http://localhost:8000/graphql/ NODE_ENV=develop webpack-dev-server --history-api-fallback --watch --port 2999 --mode development --hotOnly",
    "build-local": "API_URI=http://localhost:8000/graphql/ webpack -p --progress --verbosity verbose",
    "build-prod": "API_URI=https://api-artemis.nauticalcommerce.com/graphql/ NODE_ENV=production webpack -p --progress --verbosity verbose",
    "static": "API_URI=https://api-showcase.nauticalcommerce.com/graphql/ NODE_ENV=production webpack -p --progress --verbosity verbose",
    "test": "jest --runInBand --config .jest/config.json",
    "jest-update-snapshots": "jest -u --config .jest/config.json",
    "start": "API_URI=http://localhost:8000/graphql/ NODE_ENV=develop webpack-dev-server --history-api-fallback --port 3000 --mode development --hot",
    "start-prod": "cross-env API_URI=https://api-showcase.nauticalcommerce.com/graphql/ NODE_ENV=develop webpack-dev-server --history-api-fallback --port 3000 --mode development",
